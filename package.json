  },
  "homepage": "https://github.com/LabsStar/goddess-anime#readme",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@discordjs/builders": "^1.2.0",
    "@discordjs/rest": "^1.1.0",
